datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model userApiLimit {
  id String @id @default(cuid()) @map("_id") @db.ObjectId
  userId String @unique
  count Int @default(0)
  createAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
